{
  "name": "crossbell-ipfs-utils",
  "version": "0.0.19",
  "license": "MIT",
  "private": true,
  "repository": "github:Crossbell-Box/crossbell-ipfs-utils",
  "bugs": "https://github.com/Crossbell-Box/crossbell-ipfs-utils/issues",
  "homepage": "https://github.com/Crossbell-Box/crossbell-ipfs-utils#readme",
  "author": "Natural Selection Labs",
  "keywords": [],
  "scripts": {
    "build": "turbo run build",
    "build:pkgs": "turbo run build --filter=@crossbell/*",
    "build:release": "turbo run build:release --force",
    "bump-version": "bumpp package.json packages/*/package.json examples/*/package.json",
    "clean": "turbo run clean",
    "format": "run-p format:code format:package",
    "format:code": "prettier '@(packages|examples)/**/*.@(ts|tsx|html|json)' --write",
    "format:package": "format-package 'package.json' -w -c format-package.json",
    "lint": "eslint . --ext .ts,.tsx --fix --max-warnings 0",
    "prepare": "husky install",
    "release": "run-s build:release bump-version",
    "test": "turbo run test"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "bumpp": "^9.2.0",
    "commander": "^11.1.0",
    "esbuild": "^0.19.5",
    "esbuild-register": "^3.5.0",
    "eslint": "8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "format-package": "^7.0.0",
    "husky": "^8.0.3",
    "msw": "^2.0.2",
    "lint-staged": "^15.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "tsconfig-paths": "^4.2.0",
    "tsd": "^0.29.0",
    "tslib": "^2.6.2",
    "tsup": "^7.2.0",
    "turbo": "^1.10.16",
    "typescript": "^5.2.2"
  }
}
